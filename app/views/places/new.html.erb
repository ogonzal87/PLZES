<!-- This is the search or "Search" page.  -->
<div class="row">
	<div class="col-md-4"></div>
		<div class="col-md-4 search-bar">
		<div>
		<center><h1>What city are you visiting this time?</h1></center>
			<%= form_tag places_results_path, :method => 'get' do %>
				<%= text_field_tag :location, params[:location], class: 'form-control' %>
				<%= text_field_tag :category, params[:category], class: 'form-control' %>
				<%= submit_tag "Let's see some Plzes", class: "btn btn-primary" %>
			<% end %>
		</div>
	</div>
	<div class="col-md-4"></div>
</div>










